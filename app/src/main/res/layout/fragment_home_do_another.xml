<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rectangle_solid_gray_1_radius_12"
    tools:context=".presentation.home.view.HomeDoAnotherFragment">

    <kr.co.nottodo.view.calendar.monthly.monthlycalendarpicker.MonthlyCalendarMultiplePicker
        android:id="@+id/home_do_another_calendar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rectangle_solid_gray_1_radius_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_home_calendar_select_done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="22dp"
        android:text="@string/home_calendar_select_done"
        android:textAppearance="@style/M16"
        android:textColor="@color/gray_4_9398aa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/home_do_another_calendar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="25dp"
        android:text="@string/home_calendar_todo_description"
        android:textAppearance="@style/R12"
        android:textColor="@color/gray_4_9398aa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/home_do_another_calendar"
        app:layout_constraintStart_toStartOf="@id/home_do_another_calendar"
        app:layout_constraintTop_toBottomOf="@id/home_do_another_calendar" />


</androidx.constraintlayout.widget.ConstraintLayout>